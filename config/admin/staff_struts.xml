<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts> 
		
		<package name="setStaff" extends="struts-default">
			<action name="setStaff" class="staffAction">
				<result name="listStaff">/admin/setStaff/listStaff.jsp</result>
				<result name="chooseBranch">/admin/setStaff/chooseBranch.jsp</result>
				<result name="chooseDept">/admin/setStaff/chooseDept.jsp</result>
				<result name="addStaff">/admin/setStaff/addStaff.jsp</result>
				<result name="updateStaffBranch">/admin/setStaff/updateStaffBranch.jsp</result>
				<result name="updateStaffDept">/admin/setStaff/updateStaffDept.jsp</result>
				<result name="updateStaff">/admin/setStaff/updateStaff.jsp</result>

			</action>
		</package>
</struts>
