<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts> 
		<package name="student" extends="struts-default" >
			<action name="student" class="com.scusw.student.action.StudentAction">
				<result name="success">/student/showInfo.jsp</result>
				<result name="majorInfo">/student/showMajorInfo.jsp</result>
				<result name="notice">/student/showNotice.jsp</result>
				<result name="noticeContent">/student/showNoticeContent.jsp</result>
				<result name="update">/student/updateStudent.jsp</result>
				<result name="courseInfo">/student/queryCourseInfo.jsp</result>
				<result name="allCourseInfo">/student/selectCourses.jsp</result>
				<result name="selectCourseSuccess">/student/queryCourseInfo.jsp</result>
				<result name="selectCourseDefault">/student/selectCourseDefault.jsp</result>
				
				<result name="no_privilege">/include/noPrivilege.jsp</result>
			</action>
		</package>
</struts>
