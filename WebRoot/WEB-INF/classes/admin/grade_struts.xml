<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts> 
		
		<package name="grade" extends="struts-default">
			<action name="grade" class="gradeAction">
				<result name="success">/admin/gradeInfo/gradeInfo.jsp</result>
				<result name="failed">/index.jsp</result>
				<result name="update_success">/admin/gradeInfo/updateGrade.jsp</result>
				<result name="add_grade">/admin/gradeInfo/addGrade.jsp</result>
				<result name="no_privilege">/include/noPrivilege.jsp</result>
			</action>
		</package>
</struts>
