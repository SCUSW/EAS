<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts> 
		
		<package name="class" extends="struts-default">
			<action name="class" class="classAction">
				<result name="success">/admin/classInfo/classInfo.jsp</result>
				<result name="failed">/index.jsp</result>
				<result name="update_success">/admin/classInfo/updateClass.jsp</result>
				<result name="add_class">/admin/classInfo/addClass.jsp</result>
				<result name="no_privilege">/include/noPrivilege.jsp</result>
				
			</action>
		</package>
</struts>
